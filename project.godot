; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="CarrotFantasy"
run/main_scene="res://scene/main/MainScene.tscn"
config/features=PackedStringArray("4.0", "C#", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

SoundManager="*res://autoload/SoundManager.tscn"
SceneManager="*res://autoload/SceneManager.tscn"

[display]

window/size/viewport_width=768
window/size/viewport_height=512
window/size/window_width_override=998
window/size/window_height_override=666
window/stretch/mode="canvas_items"

[dotnet]

project/assembly_name="CarrotFantasy"

[input]

click={
"deadzone": 0.5,
"events": [Object(InputEventMouseButton,"resource_local_to_scene":false,"resource_name":"","device":-1,"window_id":0,"alt_pressed":false,"shift_pressed":false,"ctrl_pressed":false,"meta_pressed":false,"button_mask":1,"position":Vector2(79, 15),"global_position":Vector2(83, 56),"factor":1.0,"button_index":1,"pressed":true,"double_click":false,"script":null)
]
}

[layer_names]

2d_physics/layer_1="Carrot"
2d_physics/layer_2="Monster"
2d_physics/layer_3="Prop"
2d_physics/layer_4="Bullet"

[rendering]

textures/canvas_textures/default_texture_filter=2
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
